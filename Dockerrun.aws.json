{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[
        {
            "name": "client",
            "image": "natnathou/multi-client",
            "hostname": "web",
            "essential": false,
            "memory": 128
        },
        {
            "name": "server",
            "image": "natnathou/multi-server",
            "hostname": "api",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "natnathou/multi-nginx",
            "hostname": "nginx",
            "essential": true,
            "portMappings":[
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"],
            "memory": 128
        }

    ]
}